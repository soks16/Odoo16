<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="estate_property_type_tree">
            <field name="name">estate.p.type.tree</field>
            <field name="model">estate.property.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="estate_property_type_action" model="ir.actions.act_window">
            <field name="name">Properties Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">estate.property.type</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Property Types"
                  id="menu_ss2_estate_property"
                  parent="estate.menu_estate_property_type"
                  action="estate_property_type_action"
                  />

    </data>
</odoo>